function entry()
{
	// Disable input
	Interface_activateCursor(0);
	Interface_activateUserput(0);

	// Walk and say something
	Script_delaySeconds(1);
	Actor_talkEgo("What the..., whipping a tent brought me here ?");
	Script_waitForMessage();

	// Activate input
	Interface_activateCursor(1);
	Interface_activateUserput(1);
}

function exit()
{
}
